public class student {
   String surname;
   String firstname;
   char middleinitial;
   String dateofbirth;
   String studentnumber;
   String studentemailadd;
   boolean amiawesome;

    public void sayMyStudentNumber(){
        System.out.println("My student number is " + studentnumber);
    }
    public void sayMyEmailAddress(){
        System.out.println("My email address is" + studentemailadd);
    }
    public void amIawesome(){
        System.out.println("Am i awesome " + amiawesome);
    }
}

